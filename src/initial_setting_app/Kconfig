#
# For a description of the syntax of this configuration file,
# see the file kconfig-language.txt in the NuttX tools repository.
#

config EXTERNAL_ISAPP
	tristate "Initial Setting App"
	default y
	---help---
		Enable Initial Setting App

if EXTERNAL_ISAPP

config EXTERNAL_ISAPP_PROGNAME
	string "Program name"
	default "initial_setting_app"
	---help---
		This is the name of the program that will be used when the NSH ELF
		program is installed.

config EXTERNAL_ISAPP_PRIORITY
	int "isa task priority"
	default 100

config EXTERNAL_ISAPP_STACKSIZE
	int "isa stack size"
	default DEFAULT_TASK_STACKSIZE

config EXTERNAL_ISAPP_ENABLE_QR_QUALITY_OPTIMIZATION
    bool "Enable QR quality optimization"
    default n
	---help---
		Enable QR quality optimization feature in the initial setting app.

endif
